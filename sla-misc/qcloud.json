{
    "query" : {
        "has_parent" : {
            "parent_type" : "publish",
            "query" : {
                "bool" : { "must" : [
                    {"term" : { "body_type" : "publish" }},
                    {"term" : { "toplevel_domain" : "myqcloud.com" }}
                ]}
            }
        }
    },
    "aggs" : {
        "types" :{
            "terms" : { "field" : "body_type"},
            "aggs" : {
                "latency_stats" : { 
                    "stats" : { "field" : "audio_delay_diff"}
                }
            }
        }
    }
}
